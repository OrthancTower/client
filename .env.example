# Orthanc Client Configuration

# Enable/disable Orthanc client
ORTHANC_ENABLED=true

# Orthanc server URL
ORTHANC_API_URL=https://orthanc.yourcompany.com

# API Token (get this from server admin)
ORTHANC_API_TOKEN=orthanc_app123_abc...

# HTTP timeout (seconds)
ORTHANC_TIMEOUT=10

# Queue configuration (optional)
ORTHANC_QUEUE_ENABLED=false
ORTHANC_QUEUE_CONNECTION=redis
ORTHANC_QUEUE_NAME=orthanc-client
